{
  "name": "tg-modal",
  "version": "0.3.1",
  "description": "Universal controlled modals for React",
  "main": "index",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/thorgate/tg-modal.git"
  },
  "files": [
    "dist/",
    "dist-013/",
    "index.js",
    "lib.js"
  ],
  "keywords": [
    "react",
    "modal",
    "isomorphic",
    "universal"
  ],
  "author": "Thorgate <info@thorgate.eu>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/thorgate/tg-modal/issues"
  },
  "homepage": "https://github.com/thorgate/tg-modal#readme",
  "devDependencies": {
    "babel-cli": "^6.5.1",
    "babel-core": "^6.5.1",
    "babel-eslint": "^4.1.8",
    "babel-loader": "^6.2.2",
    "babel-plugin-react-version": "^0.1.0",
    "babel-preset-es2015": "^6.5.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.5.1",
    "chai": "*",
    "compass-mixins": "^0.12.7",
    "coveralls": "*",
    "css-loader": "^0.23.1",
    "dependency-check": "*",
    "doctoc": "*",
    "eslint": "^1.10.3",
    "eslint-config-airbnb": "^5.0.0",
    "eslint-plugin-react": "^3.16.1",
    "eventsource-polyfill": "^0.9.6",
    "extract-text-webpack-plugin": "^1.0.1",
    "fbjs-scripts": "^0.5.0",
    "file-loader": "^0.8.5",
    "is": "*",
    "istanbul": "*",
    "jsdom": "^8.0.2",
    "json-loader": "^0.5.4",
    "koa": "^1.1.2",
    "lodash": "^4.3.0",
    "mocha": "*",
    "node-sass": "^3.4.2",
    "postcss-loader": "^0.8.0",
    "raw-loader": "^0.5.1",
    "react-addons-test-utils": "^0.14.7",
    "react-addons-transition-group": "^0.14.7",
    "react-dom": "^0.14.7",
    "react-hot-loader": "^1.3.0",
    "react-remarkable": "^1.1.1",
    "rimraf": "*",
    "sass-loader": "^3.1.2",
    "sinon": "^1.17.3",
    "style-loader": "^0.13.0",
    "superagent": "*",
    "url-loader": "^0.5.7",
    "watch": "*",
    "webpack": "^1.12.13",
    "webpack-dev-server": "^1.14.1"
  },
  "peerDependencies": {
    "react": "^0.13.0 || ^0.14.0"
  },
  "scripts": {
    "clean": "rimraf dist",
    "lint": "eslint src test examples",
    "check": "npm run lint -s && ./node_modules/.bin/dependency-check package.json --entry src",
    "watch": "watch 'npm run build' src test",
    "test": "mocha --compilers js:babel-register -u exports -R nyan --require ./test/utils test",
    "pretest": "npm run check-engine",
    "prebuild": "npm run check-engine",
    "build": "NODE_ENV=production babel --optional runtime src -d dist && npm run build-examples",
    "build-013": "NODE_ENV=production-0.13 babel --optional runtime src -d dist-013 && npm run build-examples",
    "postbuild": "npm run test -s; npm run coverage",
    "coverage": "babel-node ./node_modules/istanbul/lib/cli cover node_modules/mocha/bin/_mocha -- -u exports --require ./test/utils test",
    "coveralls": "npm run coverage -s && coveralls < coverage/lcov.info",
    "postcoveralls": "rimraf ./coverage",
    "deploy": "git pull --rebase origin master && git push origin master",
    "patch": "npm version patch && npm publish",
    "minor": "npm version minor && npm publish",
    "major": "npm version major && npm publish",
    "prepublish": "npm run build -s ; npm run build-013 -s",
    "postpublish": "git push origin master --follow-tags",
    "toc": "doctoc --github --title \"# Changelog\" CHANGELOG.md",
    "start": "./node_modules/.bin/webpack-dev-server",
    "server": "node --harmony server.js",
    "build-examples": "NODE_ENV=production ./node_modules/.bin/webpack --config=webpack.config.prod.js",
    "postbuild-examples": "cp examples/index.html dist/examples && cp src/styles/default.scss dist/ && cp dist/examples/default.css dist && cp examples/images/favicon.png dist/examples",
    "check-engine": "node node_modules/fbjs-scripts/node/check-dev-engines.js package.json"
  },
  "devEngines": {
    "node": "4.x || 5.x",
    "npm": "2.x || 3.x"
  }
}
