{
    "presets": ["es2015", "react"],
    "plugins": [
        ["react-version", {
            version: "15"
        }],
        "transform-runtime",
        "transform-class-properties",
        "transform-object-rest-spread",
        "transform-export-extensions"
    ],

    "env": {
        "production-0.13": {
            "plugins": [
                ["react-version", {
                    version: "013"
                }]
            ]
        }
    }
}
